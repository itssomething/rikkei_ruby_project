<div class="container">
  <br>
  <h1 class="text-center">All users</h1>
  <table class="table">
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Email</th>
      <th>Number of test done</th>
      <th>Action</th>
    </tr>
    <%= render @users, tests_count: @tests_count %>
  </table>

  <div class="paginate">
    <%= paginate @users, window: 10 %>
  </div>
</div>
